generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Contact {
  id           String   @id @default(cuid())
  name         String
  email        String
  message      String
  phone        String?
  isRead       Boolean  @default(false)
  isReplied    Boolean  @default(false)
  replyMessage String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
